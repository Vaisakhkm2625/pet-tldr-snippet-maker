  output = ""

[[snippets]]
  description = "Scale an image such that its width and height is changed by the specified factors"
  command = "pnmscalefixed -xscale <x_factor> -yscale <y_factor> <path/to/input.pnm> > <path/to/output.pnm>"
  output = ""

