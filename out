  command = "bower install <local_name>=<package>#<version> --save"
  output = ""

[[snippets]]
  description = "Display help for a specific command"
  command = "bower help <command>"
  output = ""

[[snippets]]
  description = "Print group memberships for the current user"
  command = "groups"
  output = ""

