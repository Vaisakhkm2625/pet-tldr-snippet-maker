  command = "typeset <variable>=(<item_a item_b item_c>)"
  output = ""

[[snippets]]
  description = "Declare an associative array variable with the specified value"
  command = "typeset -A <variable>=(<[key_a]=item_a [key_b]=item_b [key_c]=item_c>)"
  output = ""

