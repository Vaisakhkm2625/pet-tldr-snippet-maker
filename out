[[snippets]]
  description = "Auto-detect and switch to the current platform"
  command = "sudo tlmgr platform set auto"
  output = ""

[[snippets]]
  description = "Switch to a specific platform"
  command = "sudo tlmgr platform set <platform>"
  output = ""

