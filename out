  output = ""

[[snippets]]
  description = "Check whether a [d]irectory exists"
  command = "if [[ -d <path/to/directory> ]]; then <echo ""Condition is true"">; fi"
  output = ""

[[snippets]]
  description = "Check whether a file or directory [e]xists"
