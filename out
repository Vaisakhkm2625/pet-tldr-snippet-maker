  command = "mpd --no-config"
  output = ""

[[snippets]]
  description = "Start MPD and don't detach it from the console"
  command = "mpd --no-daemon"
  output = ""

[[snippets]]
  description = "Kill the currently running MPD session"
  command = "mpd --kill"
  output = ""

